    "axios": "^0.27.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.1",
    "dynamodb": "^1.3.0",
    "http-status-codes": "^2.2.0",
    "parse-multipart": "^1.0.4",
